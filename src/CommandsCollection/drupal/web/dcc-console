#!/bin/bash

############################
# DDEV Commands Collection #
############################

## https://github.com/jackd248/ddev-commands-collection
## v<version/>
## dcc-autogenerated
. "$(dirname "$0")/../dcc-config.sh"
. "$(dirname "$0")/../scripts/dcc-colors.sh"
printf "${reset}${cyan}[DCC]${reset} DDEV Commands Collection\n"

## Description: Executes symfony console command
## Usage: console
## Example: "ddev console cache:clear" or "ddev console doctrine:migrations:migrate"
printf "${reset}${cyan}[DCC]${reset}${reset}<web> ${black}drush $@${reset}\n"
drush $@
